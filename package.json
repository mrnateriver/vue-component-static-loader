{
  "name": "vue-component-static-loader",
  "version": "0.9.2",
  "description": "A loader for Webpack that statically parses Vue class components with specified decorator and transforms them into SFC without *.vue files.",
  "main": "dist/commonjs/loader.js",
  "module": "dist/esm/loader.js",
  "types": "dist/types/loader.d.ts",
  "scripts": {
    "build": "webpack && tsc -d --declarationDir dist/types",
    "test": "ts-node -O \"{ \\\"module\\\": \\\"commonjs\\\" }\" --pretty tests/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrnateriver/vue-component-static-loader.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "class",
    "component",
    "decorator",
    "sfc"
  ],
  "author": "Evgenii Dobrovidov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrnateriver/vue-component-static-loader/issues"
  },
  "homepage": "https://github.com/mrnateriver/vue-component-static-loader#readme",
  "dependencies": {
    "vue-class-component": "https://github.com/mrnateriver/vue-class-component.git"
  },
  "devDependencies": {
    "@types/qs": "^6.5.1",
    "@types/vue": "^2.0.0",
    "@types/webpack": "^4.4.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "hash-sum": "^1.0.2",
    "qs": "^6.5.2",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-command": "^0.2.1"
  },
  "peerDependencies": {
    "vue-hot-reload-api": "^2.3.0"
  }
}
